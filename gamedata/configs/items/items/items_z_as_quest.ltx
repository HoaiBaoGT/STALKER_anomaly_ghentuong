
[as_physic_object]
GroupControlSection	= spawn_group
$spawn				= "physics\object"
$prefetch			= 8
class				= O_PHYSIC
remove_time			= 60

[as_physic_object_script_bind]:as_physic_object
script_binding		= bind_physic_object.init


;------------------------------------------------------------
;Scripted objects
;------------------------------------------------------------

[as_script_grenade]:explosive_grenade
script_binding		= bind_physic_object.init
custom_data			= scripts\as_logic\as_script_grenade.ltx

[as_script_fake_explosion]:explosive_grenade
script_binding		= bind_physic_object.init
custom_data			= scripts\as_logic\as_script_fake_explosion.ltx

[as_script_psy_generator]:explosive_barrel
script_binding		= bind_physic_object.init
visual				= dynamics\light\light_altar.ogf
custom_data			= scripts\as_logic\as_script_psy_generator.ltx
immunities_sect		= as_script_psy_generator_immunities_sect

[as_script_psy_generator_immunities_sect]
burn_immunity			= 0.0
strike_immunity			= 0.0
shock_immunity			= 0.0
wound_immunity			= 0.0
radiation_immunity		= 0.0
telepatic_immunity		= 0.0
chemical_burn_immunity	= 0.0
explosion_immunity		= 0.1
fire_wound_immunity		= 0.1

